{
  "id": "redis-master",
  "container": {
    "type": "DOCKER",
    "docker": {
      "network": "BRIDGE",
      "image": "$image",
      "portMappings": [
        {
          "containerPort": 6379,
          "servicePort": $hostport,
          "hostPort": 0,
          "protocol": "tcp"
        }
      ]
    }
  },
  "cpus": 0.1,
  "mem": 256
}


